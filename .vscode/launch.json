{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/index.ts",
			"stopOnEntry": false,
			"preLaunchTask": "compile-ts",
			"args": [
				"generate", "--template", "angularServiceProxy", "--uri", "http://localhost:8080"
			],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
			],
			"env": {
				"USE_MOCK": "0",
				"NODE_ENV": "development"
			},
			"console": "externalTerminal",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/dist",
			"smartStep": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}